<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:v4="http://vbms.vba.va.gov/external/eDocumentService/v4" xmlns:doc="http://vbms.vba.va.gov/cdm/document/v4" xmlns:cdm="http://vbms.vba.va.gov/cdm" xmlns:xop="http://www.w3.org/2004/08/xop/include">
   <soapenv:Header/>
   <soapenv:Body>
      <v4:uploadDocumentWithAssociations>
        <v4:document externalId="<%= externalId %>"
          fileNumber="<%= file_number %>"
          filename="<%= filename %>"
          docType="<%= doc_type %>"
          subject="<%= subject %>"
          veteranFirstName="<%= first_name %>"
          veteranMiddleName="<%= middle_name %>"
          veteranLastName="<%= last_name %>"
          newMail="<%= new_mail %>"
          source="<%= source %>"
          >
          <doc:receivedDt><%= receivedDt %></doc:receivedDt>
         </v4:document>
         <v4:documentContent>
            <doc:data>
              <xop:Include href="<%= filename %>" />
            </doc:data>
         </v4:documentContent>
      </v4:uploadDocumentWithAssociations>
   </soapenv:Body>
</soapenv:Envelope>
