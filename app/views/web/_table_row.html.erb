<%#
    Partial requires two arguments:
    - vacols_field: The date of a document as it appears in VACOLS
    - vbms_field: The date of a document as it appears in VBMS
%>
<% if vacols_field || vbms_field %>
    <tr>
      <td>
        <% if vbms_field == vacols_field %>
            <svg width="60" height="50" class="cf-icon-found" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 60 50"><title>found</title><path d="M57 13.3L29.9 41.7 24.8 47c-.7.7-1.6 1.1-2.5 1.1-.9 0-1.9-.4-2.5-1.1l-5.1-5.3L1 27.5c-.7-.7-1-1.7-1-2.7s.4-2 1-2.7l5.1-5.3c.7-.7 1.6-1.1 2.5-1.1.9 0 1.9.4 2.5 1.1l11 11.6L46.8 2.7c.7-.7 1.6-1.1 2.5-1.1.9 0 1.9.4 2.5 1.1L57 8c.7.7 1 1.7 1 2.7 0 1-.4 1.9-1 2.6z"/></svg>
        <% else %>
            <svg width="55" height="55" class="cf-icon-missing" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 55 55"><title>missing</title><path d="M52.6 46.9l-6 6c-.8.8-1.9 1.2-3 1.2s-2.2-.4-3-1.2l-13-13-13 13c-.8.8-1.9 1.2-3 1.2s-2.2-.4-3-1.2l-6-6c-.8-.8-1.2-1.9-1.2-3s.4-2.2 1.2-3l13-13-13-13c-.8-.8-1.2-1.9-1.2-3s.4-2.2 1.2-3l6-6c.8-.8 1.9-1.2 3-1.2s2.2.4 3 1.2l13 13 13-13c.8-.8 1.9-1.2 3-1.2s2.2.4 3 1.2l6 6c.8.8 1.2 1.9 1.2 3s-.4 2.2-1.2 3l-13 13 13 13c.8.8 1.2 1.9 1.2 3s-.4 2.2-1.2 3z"/></svg>
        <% end %>
      </td>
      <td><%= field_name %></td>
      <td>
        <% if vacols_field %>
            <%= vacols_field %>
        <% else %>
            <em>No Matching Document</em>
        <% end %>
      </td>
      <td>
        <% if vbms_field %>
            <%= vbms_field %>
        <% else %>
            <em>No Matching Document</em>
        <% end %>
      </td>
    </tr>
<% end %>
